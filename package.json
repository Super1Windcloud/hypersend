{
  "name": "HyperSend",
  "version": "7.7.7",
  "description": "An Electron application with React and TypeScript",
  "main": "./dist/main/index.js",
  "author": "superwindcloud",
  "homepage": "https://gitee.com/superwindcloud/hypersend",
  "type": "module",
  "license": "MIT",
  "private": false ,
  "scripts": {
    "native": "electron . ",
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "farm": "farm build ",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite build   && electron-forge start",
    "dev": "electron-vite dev --watch     ",
    "docker": "docker run -it -d --init --name kimi-free-api -p 8000:8000 -e TZ=Asia/Shanghai vinlic/kimi-free-api:latest && docker run -it -d --init --name deepseek-free-api -p 8888:8888  -e TZ=Asia/Shanghai  vinlic/deepseek-free-api:latest",
    "package:dry": "electron-forge package .",
    "zip": " electron-forge make  --skip-package -a x64 -p win32  --targets @electron-forge/maker-zip ",
    "make:dry": "electron-forge --skip-package   make . ",
    "make": "electron-forge make ",
    "package": "electron-vite build  &&  electron-forge package",
    "build": "npm run typecheck && electron-vite build",
    "build:dry": "  electron-vite build  ",
    "unpack": "electron-vite build && electron-builder --dir --config ",
    "unpack:dry": "electron-builder --dir --config ",
    "win_dry": "electron-vite build && electron-builder --win --config",
    "win-portable": "electron-vite  build  && electron-builder --win portable",
    "build:win": "npm run build && electron-builder --win --config ",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "gen-installer-icons": "electron-icon-builder --input=./resources/victory.png --output=build/victory --flatten",
    "clean": "rimraf  node_modules && npm install && npm run start"
  },
  "dependencies": {
    "@alicloud/ocr-api20210707": "^3.1.2",
    "@alicloud/openapi-client": "^0.4.12",
    "@alicloud/tea-util": "^1.4.10",
    "@ant-design/icons": "^5.6.1",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@fastify/cors": "^11.0.0",
    "@fastify/http-proxy": "^11.1.2",
    "@fastify/static": "^8.1.1",
    "@fastify/type-provider-typebox": "^5.1.0",
    "@fastify/websocket": "^11.0.2",
    "@gutenye/ocr-node": "^1.4.8",
    "antd": "^5.24.1",
    "antd-style": "^3.7.1",
    "clipboardy": "^4.0.0",
    "dotenv": "^16.4.7",
    "electron-updater": "^6.3.9",
    "fastify": "^5.2.1",
    "node-screenshots": "^0.2.1",
    "onnxruntime-node": "1.20.1",
    "openai": "^4.90.0",
    "styled-components": "^6.1.15"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.7.0",
    "@electron-forge/maker-deb": "^7.8.0",
    "@electron-forge/maker-squirrel": "^7.8.0",
    "@electron-forge/maker-wix": "^7.7.0",
    "@electron-forge/maker-zip": "^7.7.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@sinclair/typebox": "^0.34.28",
    "@types/node": "^22.13.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/ws": "^8.5.14",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-plugin-styled-components": "^2.1.4",
    "electron": "^34.2.0",
    "electron-vite": "^3.0.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "nodemailer": "^6.10.0",
    "prettier": "^3.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vite": "^6.1.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "7zip-bin",
      "tesseract.js",
      "ts-node",
      "@fastify/websocket",
      "@electron-forge/cli",
      "@electron-forge/maker-squirrel",
      "@electron-forge/maker-zip",
      "@electron-forge/maker-deb",
      "alicloud/tea-util",
      "@electron-forge/plugin-auto-unpack-natives",
      "alicloud/openapi-client",
      "@alicloud/ocr-api20210707",
      "canvas",
      "@gutenye/ocr-node",
      "onnxruntime-node",
      "@fastify/http-proxy",
      "@electron-forge/maker-wix",
      "@bitdisaster/exe-icon-extractor"
    ],
    "configDependencies": {
      "build-scripts-allowlist": "0.20250131.0+sha512-vuePnd+0F3PtuyFIHT1XsfOK7/BGkK1QmIMRHHPo+6LW7Wr/37QsQV3R/UztxoFWHHm9pSbCUovigSmeapfdLA=="
    },
    "onlyBuiltDependenciesFile": "node_modules/.pnpm-config/build-scripts-allowlist/common.json",
    "ignoredBuiltDependencies": [
      "@bitdisaster/exe-icon-extractor"
    ]
  },
  "peerDependencies": {
    "@sinclair/typebox": "^0.34.28"
  }
}
